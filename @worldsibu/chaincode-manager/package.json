{
  "name": "@worldsibu/chaincode-manager",
  "version": "1.2.14",
  "description": "Chaincode manager to install and run chaincodes",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "client/*"
  ],
  "license": "ISC",
  "bin": {
    "chaincode-manager": "./dist/command.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "clean:docs": "rimraf docs",
    "build": "npm run clean && tsc",
    "prepare": "npm run build",
    "lint": "tslint --fix -c '../../tslint.json' -p './tsconfig.json'",
    "predocs:generate": "npm run clean:docs",
    "docs:generate": "typedoc src --out docs --target ES6 --excludePrivate",
    "docs:serve": "http-server docs"
  },
  "dependencies": {
    "@worldsibu/chaincode": "^1.1.12",
    "commander": "^2.15.1",
    "docker-composer-manager": "^0.1.3",
    "fabric-ca-client": "^1.1.2",
    "fabric-client": "^1.1.2",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@types/node": "^10.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.3",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  }
}
