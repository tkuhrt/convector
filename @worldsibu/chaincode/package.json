{
  "name": "@worldsibu/chaincode",
  "version": "1.1.13",
  "description": "Chaincode Base to use in conjunction with controllers and models",
  "license": "ISC",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "client/*"
  ],
  "scripts": {
    "start": "node ./start.js",
    "------------ STANDARD TASKS ------------": "",
    "clean": "rimraf dist",
    "clean:docs": "rimraf docs",
    "build": "npm run clean && tsc",
    "copy:npmrc": "cp ../../npmrc dist/.npmrc",
    "lint": "tslint --fix -c '../../tslint.json' -p './tsconfig.json'",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts --reporter spec",
    "watch": "npm-scripts-watcher",
    "prepare": "npm run build",
    "predocs:generate": "npm run clean:docs",
    "docs:generate": "typedoc src --out docs --target ES6 --excludePrivate",
    "docs:serve": "http-server docs"
  },
  "dependencies": {
    "@theledger/fabric-chaincode-utils": "^2.0.9",
    "@worldsibu/chaincode-base-storage": "^0.1.7",
    "@worldsibu/chaincode-stub-storage": "^0.1.7",
    "@worldsibu/chaincode-utils": "^2.2.0",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@worldsibu/tellus-organization-ccc": "^1.1.4",
    "@worldsibu/tellus-organization-ccm": "^1.0.9",
    "mocha": "^5.0.3",
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.3",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  }
}
