{
  "name": "@worldsibu/chaincode-controller-interface",
  "version": "1.0.6",
  "description": "Chaincode Base Storage class. This is not intended to be used as a provider but as an abstract interface of one",
  "license": "ISC",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "dist/*"
  ],
  "bin": {
    "generate-controller-interface": "./dist/src/generate-interface.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "clean:docs": "rimraf docs",
    "build": "npm run clean && tsc",
    "prepare": "npm run build",
    "lint": "tslint --fix -c '../../tslint.json' -p './tsconfig.json'",
    "predocs:generate": "npm run clean:docs",
    "docs:generate": "typedoc src --out docs --target ES6 --excludePrivate",
    "docs:serve": "http-server docs"
  },
  "dependencies": {
    "@worldsibu/chaincode-errors": "^0.1.7",
    "commander": "^2.15.1",
    "ts-simple-ast": "^11.2.1",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "npm-scripts-watcher": "^1.0.2",
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.3",
    "tslint": "^5.9.1",
    "typescript": "2.8.3"
  }
}
